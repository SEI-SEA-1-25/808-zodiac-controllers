<h3><%= element.name %></h3>

<ul>
    <% element.signs.forEach(sign => { %>
        <li>
            <a href="/<%= element.name %>/<%= sign %>">
                <%= sign %>
            </a>
        </li>
    <% }) %>
</ul>

<ul>
<% element.traits.forEach(trait => { %>
    <li><%= trait %></li>
<% }) %>
</ul>